@{
  Layout = "_Layout";
}
<h3>Add a New Vendor</h3>
<form action="/vendors" method="post">
<div class="form-group">
  <label for="vendorName">Name</label>
  <input class="form-control" id="vendorName" name="vendorName" type="text">
</div>
<div class="form-group">
  <label for="vendorStreetAddress">Street Address</label>
  <input class="form-control" id="vendorStreetAddress" name="vendorStreetAddress" type="text">
</div>
<div class="form-group">
  <label for="vendorCity">City</label>
  <input class="form-control" id="vendorCity" name="vendorCity" type="text">
</div>
  <button class="btn btn-primary" type="submit">Add</button>
</form>